{
    "name": "shahidkarimi/i18-js-generator",
    "license": "MIT",
    "description": "Generate JavaScript files from laravel language files",
    "keywords": ["laravel","vue-i18n","translation","vuejs","javascript"],
    "homepage": "http://github.com/shahidkarimi/i18-js-generator",
    "authors": [
        {
            "name": "Shahid Karimi",
            "email": "me@shahidkarimi.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "illuminate/console": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|~6.0|~7.0|~8.0|~9.0|~10.0|~11.0",
        "illuminate/support": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|~6.0|~7.0|~8.0|~9.0|~10.0|~11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7"
    },
    "autoload": {
        "psr-4": {
            "ShahidKarimi\\VueInternationalizationGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShahidKarimi\\VueInternationalizationGenerator\\GeneratorProvider"
            ]
        }
    }
}
